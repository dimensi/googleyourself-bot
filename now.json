{
  "version": 2,
  "name": "searchyourself",
  "alias": ["searchyourself.now.sh"],
  "public": true,
  "builds": [
    { "src": "./src/google.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/webhook", "dest": "/src/google.js" }
  ],
  "env": {
    "WEBHOOK_PATH": "/webhook",
    "GOOGLE_BOT_TOKEN": "@googlebottoken",
    "BITLY_TOKEN": "@bitlytoken",
    "BITLY_GROUP_GUID": "@bitlygroupguid"
  }
}